			"scriptLines": [
				"source(output(",
				"          country as string,",
				"          beer_servings as string,",
				"          spirit_servings as string,",
				"          wine_servings as string,",
				"          total_litres_of_pure_alcohol as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false) ~> SourceDrinks",
				"SourceDrinks select(mapColumn(",
				"          country,",
				"          beer_servings,",
				"          spirit_servings,",
				"          wine_servings,",
